{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "outDir": "../out-tsc/app",
    "baseUrl": "./",
    "module": "es2015",
    "target": "es6",
    "lib": [
      "es2016",
      "dom"
    ],
    "types": [],
    "paths": {
      "@angular/*": ["../node_modules/@angular/*"] 
    }
  },
  "exclude": [
    "test.ts",
    "**/*.spec.ts"
  ]
}
